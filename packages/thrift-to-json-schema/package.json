{
  "name": "@trasm/thrift-to-json-schema",
  "version": "0.0.1",
  "description": "@trasm/thrift-to-json-schema",
  "author": "cjinhuo",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "publishConfig": {
    "main": "dist/index.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-s esm:watch build:watch",
    "esm": "tsc -b ./tsconfig.json",
    "esm:watch": "tsc -b -w ./tsconfig.json",
    "lint": "eslint ./src/**",
    "lintHelper": "eslint -h",
    "test": "jest --config jest.config.cjs",
    "build": "rollup -c rollup.config.js",
    "build:cjs": "rollup -c ./rollups/iife.rollup.mjs",
    "build:watch": "rollup -c  ./rollups/cjs.rollup.mjs --watch",
    "batch-rollup": "batch-rollup -f ./rollups",
    "help": "rollup -h"
  },
  "dependencies": {
    "@trasm/rollup": "workspace:*",
    "@trasm/shared": "workspace:*",
    "@trasm/tsconfig": "workspace:*",
    "@creditkarma/thrift-parser": "^2.0.0",
    "@sinclair/typebox": "^0.25.2"
  }
}